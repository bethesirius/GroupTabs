{
	"manifest_version": 2,

	"name": "Group Tabs",
	"description": "This extension shows a Google Image search result for the current page",
	"version": "1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
	"default_title": "Group Tabs"
	},
	"background": {
		"scripts": ["background.js"]
	},
	"permissions": [
		"activeTab","tabs",
	]
}